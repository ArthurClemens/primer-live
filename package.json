{
  "name": "primer-live",
  "version": "0.1.3",
  "description": "JavaScript and CSS for primer_live",
  "types": "dist",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./index.ts",
  "module": "./dist/primer-live.js",
  "exports": {
    ".": {
      "import": "./dist/primer-live.js",
      "default": "./dist/primer-live.js"
    },
    "./primer-live.css": {
      "import": "./dist/primer-live.css",
      "default": "./dist/primer-live.css"
    }
  },
  "scripts": {
    "dist:clean": "rm -rf ./dist/*",
    "build:minified": "esbuild src/index.ts --bundle --minify --target=es2020,chrome58,edge18,firefox57,node12,safari11 --outfile=dist/primer-live.min.js",
    "build:esm": "esbuild src/index.ts --bundle --format=esm --outfile=dist/primer-live.js",
    "build:types": "./node_modules/typescript/bin/tsc --p tsconfig.gen.json",
    "build": "npm run dist:clean && npm run build:esm && npm run build:minified && npm run build:types"
  },
  "dependencies": {
    "dialogic-js": "^0.2.8"
  },
  "devDependencies": {
    "esbuild": "^0.15.11",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArthurClemens/primer-live.git"
  },
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://arthurclemens.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArthurClemens/primer-live/issues"
  },
  "homepage": "https://github.com/ArthurClemens/primer-live"
}
